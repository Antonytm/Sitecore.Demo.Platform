<?xml version="1.0"?>
<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/"
               xmlns:integrations="http://www.sitecore.net/xmlconfig/integrations/">
	<sitecore>
		<contentSearch>
			<indexConfigurations>
				<defaultSolrIndexConfiguration>
					<documentOptions>
						<fields hint="raw:AddComputedIndexField">
							<field fieldName="eventdate" returnType="string">Sitecore.HabitatHome.Feature.ExperienceAccelerator.Indexing.EventDateComputedField, Sitecore.HabitatHome.Feature.ExperienceAccelerator</field>
						</fields>
					</documentOptions>
				</defaultSolrIndexConfiguration>
			</indexConfigurations>
		</contentSearch>

	</sitecore>
</configuration>