<configuration xmlns:patch="http://www.sitecore.net/xmlconfig/"                                          
               xmlns:unicorn="http://www.sitecore.net/xmlconfig/unicorn/">
    <sitecore unicorn:require="On">
        <unicorn>
            <configurations>
                <configuration name="Project.HabitatHome.Corporate.Website" description="Habitat Corporate content" dependencies="">
                    <targetDataStore physicalRootPath="$(sourceFolder)\Project\HabitatCorporate\serialization" useDataCache="false" singleInstance="true" />
                    <predicate type="Unicorn.Predicates.SerializationPresetPredicate, Unicorn" singleInstance="true">
                        <include name="Templates" database="master" path="/sitecore/templates/Project/Habitat Sites/Habitat Corporate" />
                        <include name="Layouts" database="master" path="/sitecore/layout/Layouts/Project/Habitat Sites/Habitat Corporate" />
                        <include name="Content" database="master" path="/sitecore/content/Habitat Corporate" />
                    </predicate>
                </configuration>
            </configurations>
        </unicorn>
    </sitecore>
</configuration>