  <add name="messaging" connectionString="Data Source=sql;Initial Catalog=sc_Messaging;User ID=${SITECORE_SQL_USERNAME};Password=${SITECORE_SQL_PASSWORD}" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
  <add name="experienceforms" connectionString="Data Source=sql;Initial Catalog=sc_ExperienceForms;User ID=${SITECORE_SQL_USERNAME};Password=${SITECORE_SQL_PASSWORD}" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
  <add name="exm.master" connectionString="Data Source=sql;Initial Catalog=sc_EXM.Master;User ID=${SITECORE_SQL_USERNAME};Password=${SITECORE_SQL_PASSWORD}" xdt:Transform="SetAttributes" xdt:Locator="Match(name)" />
